<div class="row">
	<div class="col-lg-6">
		<div class="widget navy-bg no-padding">
			<div class="p-m">
				<h1 class="m-xs">Dashboard</h1>

				<h3 class="font-bold no-margins">
					ASP.Core 5.0
				</h3>
				<small>Projemizin bu panelinde Admin işlemleri bir area yapısından gerçekleşmektedir.
				</small>
			</div>
			<div class="flot-chart">
				<div class="flot-chart-content" id="flot-chart1"></div>
			</div>
		</div>
	</div>
	<div class="col-lg-3">
		<div class="widget lazur-bg no-padding">
			<div class="p-m">
				<h1 class="m-xs">Son Eklene Blog</h1>
				<h3 class="font-bold no-margins">
					@ViewBag.lasBlogName
				</h3>
				<small></small>
			</div>
			<div class="flot-chart">
				<div class="flot-chart-content" id="flot-chart2"></div>
			</div>
		</div>
	</div>
	<div class="col-lg-3">
		<div class="widget yellw-bg no-padding">
			<div class="p-m">
				<h1 class="m-xs">$ 50,992</h1>

				<h3 class="font-bold no-margins">
					Half-year revenue margin
				</h3>
				<small>Sales marketing.</small>
			</div>
			<div class="flot-chart">
				<div class="flot-chart-content" id="flot-chart3"></div>
			</div>
		</div>
	</div>
</div>